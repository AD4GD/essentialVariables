        skos:prefLabel      "Essential Biodiversity Variables - codelist scheme"@en ;
        dcat:theme          base: ;
        rdfs:seeAlso        base:EBV ;
        skos:hasTopConcept  <http://w3id.org/ad4gd/ev/ebv/Geneticcomposition> ;
