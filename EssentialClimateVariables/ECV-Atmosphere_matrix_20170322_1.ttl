base:ecv  rdf:type          skos:ConceptScheme ;
        rdf:type            dcat:Dataset ;
        dct:description     "Essential Climate Variables (ECVs) are defined as physical, chemical or biological variables or groups of linked variables that critically contributes to the characterization of Earth’ s climate. They are classified in three mean areas: Atmosphere, Land and Ocean"@en ;
        dct:identifier      "ECV"^^rdfs:Literal ;
        dct:title           "Essential Climate Variables - codelist scheme"@en ;
        rdfs:isDefinedBy    <https://gcos.wmo.int/en/essential-climate-variables/> ;
        skos:definition     "Essential Climate Variables (ECVs) are defined as physical, chemical or biological variables or groups of linked variables that critically contributes to the characterization of Earth’ s climate. They are classified in three mean areas: Atmosphere, Land and Ocean"@en ;
        skos:prefLabel      "Essential Climate Variables - codelist scheme"@en ;
        dcat:theme          <http://w3id.org/ad4gd/ev/> ;
        rdfs:seeAlso        base:ECV ;
        skos:hasTopConcept  <https://w3id.org/ad4gd/ev/ecv/Surface> ;
        skos:hasTopConcept  <https://w3id.org/ad4gd/ev/ecv/UpperAtmosphere> ;
        skos:hasTopConcept  <https://w3id.org/ad4gd/ev/ecv/AtmosphericComposition> .

base:ECV  rdf:type        owl:Class ;
