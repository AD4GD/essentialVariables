        skos:prefLabel      "Essential Climate Variables - codelist scheme"@en ;
        dcat:theme          <http://w3id.org/ad4gd/ev/> ;
        rdfs:seeAlso        base:ECV ;
        skos:hasTopConcept  <https://w3id.org/ad4gd/ev/ecv/Physical> ;
        skos:hasTopConcept  <https://w3id.org/ad4gd/ev/ecv/Biogechemical> ;
        skos:hasTopConcept  <https://w3id.org/ad4gd/ev/ecv/Biological/Ecosystems> .

